{
  "name": "saasquatch-docs",
  "description": "Referral SaaSquatch public documentation",
  "version": "0.0.1",
  "license": "APACHE",
  "repository": {
    "type": "git",
    "url": "http://github.com/saasquatch/saasquatch-docs.git"
  },
  "engine": {
    "node": ">= v6.3.1"
  },
  "scripts": {
    "production": "gulp production && npm run webpack",
    "dev": "gulp & gulp metalsmithDev & npm run webpack & wait",
    "local": "npm run watch & NODE_ENV=development nodemon --exec 'npm run dev && npm run browser_sync_reload'",
    "server": "npm run watch & NODE_ENV=development nodemon --exec 'gulp metalsmithDev && npm run browser_sync_reload'",
    "watch": "npm run browser_sync & node contentful-watch.js",
    "webpack": "webpack ./assets/js/docs.js ./build/assets/js/bundle.js",
    "superstatic": "superstatic build --port $PORT --config firebase.json",
    "browser_sync": "browser-sync start --server build --port $PORT --host $IP",
    "browser_sync_reload": "browser-sync reload --port $PORT --host $IP",
    "gulp": "gulp",
    "heroku-postbuild": "gulp && npm run webpack"
  },
  "dependencies": {
    "browser-sync": "^2.13.0",
