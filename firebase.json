{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [{
      "type": 301,
      "source": "/squatchjs",
      "destination": "/developer/squatchjs/v1/reference"
    }, {
      "type": 301,
      "source": "/theming",
      "destination": "/themes"
    }, {
      "type": 301,
      "source": "/api-guide",
      "destination": "/guides"
    }, {
      "type": 301,
      "source": "/faq/general",
      "destination": "/faq"
    }, {
      "type": 301,
      "source": "/pricing",
      "destination": "http://www.referralsaasquatch.com/pricing/"
    }, {
      "type": 301,
      "source": "/contact",
      "destination": "http://www.referralsaasquatch.com/contact-us/"
    }, {
      "type": 301,
      "source": "/about",
      "destination": "http://www.referralsaasquatch.com/about/"
    }, {
      "type": 301,
      "source": "/squatchjs/issues",
      "destination": "/squatchjs/issue"
    }, {
      "type": 301,
      "source": "/segment-io",
      "destination": "/segment"
    }, {
      "type": 301,
      "source": "/how-it-works",
      "destination": "/success"
    }, {
      "type": 301,
      "source": "/faq/pricing",
      "destination": "http://www.referralsaasquatch.com/pricing/"
    }]
  }
}
