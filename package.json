    "handlebars": "^4.0.5",
    "highlight.js": "^9.5.0",
    "jquery": "^3.1.0",
    "jquery-jsonview": "^1.2.3",
    "jquery.mmenu": "^5.6.5",
    "js-yaml": "^3.0.2",
    "json-schema-resolve-allof": "^1.1.1",
